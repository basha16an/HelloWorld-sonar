pipeline{
    agent any 
    stages {
        stage('Sync'){
            steps{
                echo "echo'hello this is Sync'"
            }
        }
         stage('Build'){
               steps{
                sh 'printenv'
                
                }
                echo "echo'hello this is Build'"
            
        }
       
         stage('Bundle'){
            steps {
                
                echo "echo'hello this is Bundle'"
            }
        }
    }
     post{
            always{
                                       echo "Failure"
               
                
            }
        }
}
