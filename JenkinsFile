pipeline{
    agent any 
    stages {
        stage('Sync'){
            steps{
                echo "echo'hello this is Sync'"
            }
        }
         stage('Build'){
            steps {
                echo 'printenv'
                }

            
        }
       
         stage('Bundle'){
            steps {
                echo "bundle"
             
            }
        }
    }
     post{
            always{
                                       echo "Failure"
               
                
            }
        }
}
